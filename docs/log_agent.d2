direction: right

LogAgentModule: {
  style: {
    fill: "#E8F0FE"
    stroke: "#1A73E8"
  }

  "dspy.ReAct": {
    "signature: AgentSignature"
    "max_iters: 10"
  }

  "ToolCallCallback": {
    "wraps ToolUsageTracker"
    "captures tool calls"
  }

  "ToolUsageTracker": {
    "stores registered vars"
    "get_final_output_vars()"
  }
}

"Python REPL tool": {
  style: {
    fill: "#E6F4EA"
    stroke: "#137333"
  }

  "python_repl": {
    "persistent session"
    "register_for_final_output(...)"
  }

  "REPL sub-tools": {
    "fetch_log_data(path)"
    "get_available_files()"
  }
}

"dspy.ReAct" -> "python_repl": "tool use (ReAct)"
"python_repl" -> "REPL sub-tools": "calls"
"dspy.ReAct" -> "ToolCallCallback": "callbacks"
"ToolCallCallback" -> "ToolUsageTracker": "records"
"ToolUsageTracker" -> "dspy.ReAct": "registered vars\nfor final answer"
